import java.util.Scanner;
public class SquaresSum {
    public static void main(String[] var0) {
      Scanner sc = new Scanner(System.in);
      System.out.printf("Enter the lower bounds and upper bounds : ");
      int a = sc.nextInt();
      int b = sc.nextInt();
      int sum = 0;

      for(int i = a; i <= b; i++) {
         sum = sum + i*i;
      }
      System.out.printf("The sum of the squares is : %d\n", sum);
   }
}
