import java.util.Scanner;
public class swap2Var {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        System.out.print("Enter the value of a : ");
        int a = sc.nextInt();
        System.out.print("Enter the value of b : ");
        int b = sc.nextInt();
        System.out.printf("The values of a and b before swapping are %d and %d\n",a,b);
        a = a+b;
        b = a-b;
        a = a-b;
        System.out.print("The values of a and b after swapping are "+a+" and "+b+"\n");
    }
}
